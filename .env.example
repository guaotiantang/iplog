# 应用配置
NODE_ENV=development
PORT=3000

# 数据库配置
DB_PATH=./data/iplog.db

# 默认配置
DEFAULT_IP_TIMEOUT=3600
DEFAULT_AUTO_CLEANUP_INTERVAL=86400

# 日志配置
LOG_LEVEL=info

# CORS配置
CORS_ORIGIN=*

# 安全配置（生产环境必须设置）
# JWT_SECRET=your-secret-key-here
# SESSION_SECRET=your-session-secret-here

# 可选：外部数据库配置（如果不使用SQLite）
# DATABASE_URL=postgresql://username:password@localhost:5432/iplog
# REDIS_URL=redis://localhost:6379

# 生产环境配置示例
# NODE_ENV=production
# PORT=3000
# DB_PATH=/app/data/iplog.db
# LOG_LEVEL=warn